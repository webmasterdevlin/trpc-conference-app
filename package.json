{
  "name": "my-app",
  "version": "10.21.1",
  "private": true,
  "scripts": {
    "generate": "prisma generate",
    "prisma-studio": "prisma studio",
    "db-seed": "prisma db seed",
    "db-reset": "prisma migrate dev reset",
    "dx:next": "run-s migrate-dev db-seed && next dev",
    "dx:prisma-studio": "pnpm prisma-studio",
    "dx": "run-p dx:* --print-label",
    "dev": "pnpm dx:next",
    "prebuild": "run-s generate migrate",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --report-unused-disable-directives --report-unused-disable-directives src",
    "lint-fix": "pnpm lint --fix",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "test": "run-s test:*",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test-dev": "start-server-and-test dev http://127.0.0.1:3000 test",
    "test-start": "start-server-and-test start http://127.0.0.1:3000 test",
    "postinstall": "pnpm generate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^3.10.0",
    "@prisma/client": "^6.2.1",
    "@trpc/client": "^10.45.2",
    "@trpc/next": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "clsx": "^1.2.1",
    "next": "^13.5.8",
    "next-auth": "^4.24.11",
    "postcss": "8.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "superjson": "^1.13.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.18",
    "@types/react-dom": "18.3.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "^13.5.8",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "prisma": "^6.2.1",
    "start-server-and-test": "^2.0.10",
    "tailwindcss": "3.4.17",
    "tsx": "^3.14.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vitest": "^3.0.3"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
